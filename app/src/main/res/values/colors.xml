    <color name="darkergray">@android:color/black</color>
    <color name="moresemitransparent">#99FFFFFF</color>
    <color name="dialogBackGround">#99000000</color>
    <color name="white">@android:color/white</color>
    <color name="outerCircle">#b2b0b2</color>
    <color name="eqsetingtxtcolor">#363636</color>
    <color name="selector_make_it_cool_button">#87CEFA</color>
    <color name="cardview_dark_background">#FF424242</color>
    <color name="cardview_light_background">#FFFFFFFF</color>
    <color name="cardview_shadow_end_color">#03000000</color>
    <color name="cardview_shadow_start_color">#37000000</color>


    <color name="red_300">#e57373</color>
    <color name="green_300">#81c784</color>
    <color name="grey_200">#eeeeee</color>
    <color name="grey_300">#e0e0e0</color>
